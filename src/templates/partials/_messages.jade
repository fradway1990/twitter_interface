div(class='activity-col col-xs-12 col-sm-12 col-md-4 col-lg-4')
	div(class='activity-label')
		h2 Messages
		i(class="fa fa-caret-down" aria-hidden="true")
					
	div(class='activity-container')
		ul(class='messages')
			li(class='message-me')
				div(class='img-container')
				div(class='message-container')
					div(class='message')
						| Hello Darkness my old friend. It's nice to see you again.
					div(class='timestamp')
						|3 hours ago
							
			li(class='message-other')
				div(class='img-container')
				div(class='message-container')
					div(class='message')
						| Hello Darkness my old friend. It's nice to see you again.
					div(style='clear:both')
					div(class='timestamp')
						|3 hours ago